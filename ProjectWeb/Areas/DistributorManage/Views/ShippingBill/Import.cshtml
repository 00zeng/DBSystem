@* 运费信息导入 *@
@{
    ViewBag.Title = "运费信息导入";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}

<div class="box-header widt-border">
    <h3 class="box-title">运费信息导入</h3>
	<h3 class="box-title pull-right"><a href="/HttpHandler/TemplateDownload.ashx?name=导入模板-运费模板.xlsx" rel="external nofollow"><i class='fa  fa-download'></i>模板下载</a></h3>
</div>
<div class="panel-body" style="background:white;">

        <div class="box box-default">
		
            <div class="box-body">
                @*<div class="fixed-table-toolbar btn-group">
                    <span class="form-control">选择发货时间</span>
                    <input id="shippingTime" class="input-group form-control" readonly="readonly" onfocus="WdatePicker();" />
                    
                </div> *@
                <div id="toolbar" class="btn-group">                              
                    <button class="btn btn-primary" type="button" onclick="$('#FileInput')[0].click()"><i class="glyphicon glyphicon-folder-open"></i>选择文件</button>
                    <div class="input-group" onclick="$('#FileInput')[0].click()">
                        <input type="text" class="form-control" placeholder="导入文件名" readonly="readonly" id="excelfile">
                    </div>
                </div>
                <table id="tb_table" style="table-layout:fixed;"></table>
                <input type="file" id="FileInput" hidden="hidden" style="display: none;" onchange="importfile(this)" />
            </div>
        </div>

    </div>
    <div class="box-footer">
        <button type="submit" id="btnSubmit" class="btn btn-primary pull-right" onclick="submitForm()">提交</button>
        <button id="btnReturn" class="btn btn-default pull-right">返回</button>
    </div>

<script src="~/Content/js/detail/DistributorManage/ShippingBill/import.js"></script>
