@{
    ViewBag.Title = "个人信息 ";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}
<div class="panel-body" style="background: white;">
    <form id="form1">
        <div class="box box-danger">
            <div class="box-header widt-border">
                <h3 class="box-title">经销商信息</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" onclick="EditDistributorInfo()">
                        <i class="fa fa-pencil" title="修改经销商信息"></i>
                    </button>
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body tab-content">
                <div class="tab-pane active" id="tab_1">
                    <div class="row">
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>经销商名称</dt>
                            <dd>
                                <span id="name"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>V2报量名称</dt>
                            <dd>
                                <span id="name_v2"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>快捷编码</dt>
                            <dd>
                                <span id="code" name="code"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>所属代理商</dt>
                            <dd>
                                <span id="agent" name="agent"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>潜在售点</dt>
                            <dd>
                                <span id="potential_salepoint"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>连锁体系</dt>
                            <dd>
                                <span id="system_chain"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>TOP层级客户</dt>
                            <dd>
                                <span id="top_customers"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>体系调拨</dt>
                            <dd>
                                <span id="system_allocation"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>地区属性</dt>
                            <dd>
                                <span id="locale_attribute"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>客户类别</dt>
                            <dd>
                                <span id="customer_category"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>客户级别</dt>
                            <dd>
                                <span id="customer_level"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>经销商属性</dt>
                            <dd>
                                <span id="distributor_attribute"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>运营商属性</dt>
                            <dd>
                                <span id="sp_attribute"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>经营品牌</dt>
                            <dd>
                                <span id="brand"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>年销售规模</dt>
                            <dd>
                                <span id="annual_sales_volume"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>经营模式</dt>
                            <dd>
                                <span id="operation_mode"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>价格级别</dt>
                            <dd>
                                <span id="price_level"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>所属机构</dt>
                            <dd>
                                <span id="company_name"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>所属区域</dt>
                            <dd>
                                <span id="area_l2_name"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>所在省市</dt>
                            <dd>
                                <span id="city" name="city"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>地址</dt>
                            <dd>
                                <span id="address" name="address"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>电话</dt>
                            <dd>
                                <span id="phone" name="phone"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>传真</dt>
                            <dd>
                                <span id="fax"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>联系人</dt>
                            <dd>
                                <span id="contact_name"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>联系人电话</dt>
                            <dd>
                                <span id="contact_phone"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>创建人</dt>
                            <dd>
                                <span id="creator_name" class="creator_name"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>创建时间</dt>
                            <dd>
                                <span id="create_time"></span>
                            </dd>
                        </dl>
                        <dl class="col-sm-6 col-lg-4 dl-horizontal">
                            <dt>备注</dt>
                            <dd>
                                <span id="note"></span>
                            </dd>
                        </dl>
                    </div>
                </div>
            </div>
        </div>
        <div class="box box-warning">
            <div class="box-header widt-border">
                <h3 class="box-title">账户信息</h3>
                <div class="box-tools pull-right">
					<button type="button" class="btn btn-box-tool" onclick="EditAccountInfo()">
                        <i class="fa fa-pencil" title="修改经销商账户信息"></i>
                    </button>
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body">
                <div class="row">
                    <dl class="col-sm-6 col-lg-4 dl-horizontal">
                        <dt>系统账户</dt>
                        <dd>
                            <span id="account" name="account"></span>
                        </dd>
                    </dl>
                    <dl class="col-sm-6 col-lg-4 dl-horizontal">
                        <dt>账户状态</dt>
                        <dd>
                            <span id="account_state" name="account_state"></span>
                        </dd>
                    </dl>
                </div>
            </div>
        </div>
        <div class="box box-success collapsed-box">
            <div class="box-header widt-border">
                <h3 class="box-title">图片信息</h3>
                <div class="box-tools pull-right">
                    @*修改图片 无法上传 *@
					@*
                    <button type="button" class="btn btn-box-tool" onclick="EditPictureInfo()">
                        <i class="fa fa-pencil" title="修改经销商图片信息"></i>
                    </button>*@
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-plus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body no-padding">
                <div class="row">
                    <div class="col-md-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">营业执照</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>


                    <div class="col-md-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">门店照片</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>

                    <div class="col-md-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">身份证</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                </div>

                <div class="row">
                    <div class="col-md-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">合同</h3>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>


                    <div class="col-md-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">其他</h3>
                            </div>
                            <!-- /.box-header -->
                              <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="box-footer">
        @*<button type="submit" class="btn btn-danger pull-right" onclick="SetClose()">停业</button>*@
        @*<button  class="btn btn-primary pull-right" onclick="Edit()">修改</button>*@
        <a class="btn btn-default pull-right" href="/DistributorManage/DistributorManage/Index">返回</a>
    </div>
</div>

<script src="~/Content/js/detail/DistributorManage/distributor/show.js"></script>

