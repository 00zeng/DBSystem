@{
    ViewBag.Title = "经销商图片信息修改";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}
<link href="~/Content/js/cityPicker/css/city-picker.css" rel="stylesheet" />

<div class="panel-body" style="background: white;">
    <form id="form1">
       
        <div class="box box-success">
            <div class="box-header widt-border">
                <h3 class="box-title"><span id="name"></span>图片信息修改</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body no-padding">
                <div class="row">
                    <div class="col-sm-6 col-lg-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">营业执照</h3>
                                <button type="button" class="btn bg-primary pull-right" add="upload">上传图片</button>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>

                    <div class="col-sm-6 col-lg-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">门店照片</h3>
                                <button type="button" class="btn bg-primary pull-right" add="upload">上传图片</button>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>

                    <div class="col-sm-6 col-lg-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">身份证</h3>
                                <button type="button" class="btn bg-primary pull-right" add="upload">上传图片</button>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>

                    <div class="col-sm-6 col-lg-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">合同</h3>
                                <button type="button" class="btn bg-primary pull-right" add="upload">上传图片</button>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>

                    <div class="col-sm-6 col-lg-4">
                        <div class="box box-solid">
                            <div class="box-header with-border">
                                <h3 class="box-title">其他</h3>
                                <button type="button" class="btn bg-primary pull-right" add="upload">上传图片</button>
                            </div>
                            <!-- /.box-header -->
                            <div class="box-body" name="picbox" style="display: none">
                                <div class="row mg-t" name="row"></div>
                            </div>
                            <!-- /.box-body -->
                        </div>
                        <!-- /.box -->
                    </div>
                </div>
            </div>
        </div>
    </form>
    <div class="box-footer">
        <button type="submit" class="btn btn-primary pull-right" onclick="submitForm()">提交</button>
        <a type="submit" class="btn btn-default pull-right" href="javascript:history.go(-1);">返回</a>
    </div>
</div>

<script src="~/Content/js/cityPicker/city-picker.data.js"></script>
<script src="~/Content/js/cityPicker/city-picker.js"></script>
<script src="~/Content/js/detail/DistributorManage/distributor/edit_picture_info.js"></script>