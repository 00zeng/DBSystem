@{
    ViewBag.Title = "KPI历史";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}
<style type="text/css">
    tbody table td {
        padding: 0;
    }

    .radio + .radio {
        margin-top: 10px;
    }

    .table-bordered {
        border-radius: 4px;
    }
</style>
<form id="form1">
    <div class="box-header">
        <h4><span id="position_name"></span>KPI评分</h4>
        <div>
            <div class="col-xs-12 col-sm-4 col-lg-4">
                <div class="input-group  margin-bottom">
                    <span class="input-group-addon no-border">姓名：</span>
                    <span type="text" id="name" class="form-control "></span>
                </div>
            </div>
            <div class="col-xs-12 col-sm-4 col-lg-4">
                <div class="input-group  margin-bottom">
                    <span class="input-group-addon no-border">等级：</span>
                    <span type="text" id="grade" class="form-control "></span>
                </div>
            </div>
            <div class="col-xs-12 col-sm-4 col-lg-4">
                <div class="input-group formValue margin-bottom">
                    <span class="input-group-addon no-border">考核周期：</span>
                    <span id="month" class="form-control format_date_ym"></span>
                </div>
            </div>
        </div>
    </div>

    <div class="box-body">
        <div class="row">
            <div class="col-lg-5 col-sm-12">
                <div class="box box-danger">
                    <div class="box-header with-border">
                        <h4 class="pull-left" style="margin-right:32px">月度考核</h4>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body" style="height:400px">
                        <div class="form-group col-sm-12">
                            <label>正常机</label>
                            <div class="row">
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        销量：<label id="normal_count"></label>台
                                    </span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        批发价总额：<label id="normal_amount_w"></label>元
                                    </span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        零售价总额：<label id="normal_amount_r"></label>元
                                    </span>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-sm-12">
                            <label>买断机</label>
                            <div class="row">
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        销量：<label id="buyout_count"></label>台
                                    </span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        批发价总额：<label id="buyout_amount_w"></label>元
                                    </span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        买断价总额：<label id="buyout_amount"></label>元
                                    </span>

                                </div>
                            </div>
                        </div>

                        <div class="form-group col-sm-12">
                            <label>总计</label>
                            <div class="row">
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        销量：<label id="total_count"></label>台
                                    </span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        批发价总额：<label id="total_amount_w"></label>元
                                    </span>
                                </div>
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <span>
                                        零售价总额：<label id="total_amount_r"></label>元
                                    </span>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-sm-12">
                            <div class="row">
                                <div class="col-xs-12 col-sm-4 col-lg-4">
                                    <label>KPI总金额：<span id="kpi_total"></span>元 </label>
                                </div>
                            </div>
                        </div>

                        <div class="form-group col-sm-12">
                            <div class="row">
                                <div class="col-xs-12 col-sm-5 col-lg-12" id="kpi_total_a_div">
                                    <div class="input-group margin-bottom">
                                        <span class="input-group-addon no-border text-blue">调整金额：</span>
                                        <div class="input-group">
                                            <div class="input-single formValue">
                                                <span class="form-control text-center required text-blue" id="kpi_total_a"></span>
                                            </div>
                                            <span class="input-group-addon text-blue">元</span>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-12 col-sm-7 col-lg-12" id="note_div">
                                    <div class="input-group margin-bottom">
                                        <span class="input-group-addon no-border">调整说明：</span>
                                        <textarea id="note" class="form-control" disabled></textarea>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div class="col-lg-7 col-sm-12">
                <div class="box box-warning">
                    <div class="box-header with-border">
                        <h4 class="title">销售详情</h4>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        <div class="row">
                            <button type="button" id="exportExcel" class="btn btn-warning">
                                <span class="glyphicon glyphicon-export" aria-hidden="true"></span>导出
                            </button>
                            <div class="col-lg-3 col-sm-3">
                                <input class="form-control" id="phone_sn" type="text" placeholder="请输入串码">
                            </div>
                            <div class="col-lg-3 col-sm-3">
                                <input class="form-control" id="model" type="text" placeholder="请输入型号">
                            </div>
                            <div class="col-lg-3 col-sm-3">
                                <select class="form-control" id="sale_type" name="sale_type">
                                    <option value="10">全部</option>
                                    <option value="0">已出库</option>
                                    <option value="1">正常销售</option>
                                    <option value="2">买断</option>
                                </select>
                            </div>
                            <div class="col-lg-1 col-sm-1">
                                <i id="querySubmit" class='fa fa-search btn btn-primary'></i>
                            </div>
                        </div>
                        <table id="gridList"></table>@*style="table-layout:fixed"*@
                    </div>
                </div>
            </div>
            <div class="col-lg-12 col-sm-12">
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h4 class="title">考核政策</h4>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        <div class="col-lg-6">
                            <div class="form-group col-sm-12">
                                <label>考核内容</label>
                                <div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input id="target_content" name="target_content" type="radio" value="1" disabled>按实销量
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="target_content" type="radio" value="2" disabled>按下货量
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group col-sm-12">
                                <label>考核模式</label>
                                <div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input id="target_mode" name="target_mode" type="radio" value="1" disabled>按完成率
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="target_mode" type="radio" value="2" disabled>按台数
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="target_mode" type="radio" value="3" disabled>按零售价
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="target_mode" type="radio" value="5" disabled>按批发价
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group col-sm-12">
                                <label>正常机提成</label>
                                <div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input id="normal_rebate_mode" name="normal_rebate_mode" type="radio" value="1" disabled>每台固定金额
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="normal_rebate_mode" type="radio" value="2" disabled>每台批发价比例
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="normal_rebate_mode" type="radio" value="3" disabled>每台零售价比例
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="normal_rebate_mode" type="radio" value="4" disabled>固定总金额
                                        </label>
                                    </div>
                                </div>
                            </div>

                            <div class="form-group col-sm-12">
                                <label>买断机提成</label>
                                <div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input id="buyout_rebate_mode" name="buyout_rebate_mode" type="radio" value="1" disabled>每台固定金额
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="buyout_rebate_mode" type="radio" value="2" disabled>每台批发价比例
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="buyout_rebate_mode" type="radio" value="3" disabled>每台买断价比例
                                        </label>
                                    </div>
                                    <div class="radio col-sm-3">
                                        <label>
                                            <input name="buyout_rebate_mode" type="radio" value="4" disabled>固定总金额
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div id="forTargetMode1" style="display:none" class="form-group col-sm-12">
                                <label>目标销量</label>
                                <div class="input-group col-sm-3  col-lg-6 formValue">
                                    <span id="activity_target" name="activity_target" class="form-control text-center"></span>
                                    <span class="input-group-addon">台</span>
                                </div>
                            </div>

                        </div>
                        <div class="form-group col-sm-12 col-lg-6">
                            <div class="box-body">
                                <div class="form-group">
                                    <table id="rebateMode1" class="table table-bordered text-center">
                                        <tr>
                                            <th class="col-sm-4 bg-gray-light"><span name="target_mode_change">完成率（%）</span></th>
                                            <th class="col-sm-4 bg-gray-light">正常机提成<span name="rebate_mode_change"></span></th>
                                            <th class="col-sm-4 bg-gray-light">买断机提成<span name="rebate_mode_change2"></span></th>
                                        </tr>
                                    </table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
<div class="box-footer">
    @*<button type="submit" class="btn btn-primary pull-right" onclick="submitForm()">提交</button>*@
    <button type="submit" class="btn btn-default pull-right" onclick="javascript:history.back(-1);">返回</button>
</div>

<script src="~/Content/js/detail/FinancialAccounting/SalesKPI/show.js"></script>
