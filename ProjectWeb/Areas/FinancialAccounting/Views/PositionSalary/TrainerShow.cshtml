@{
    ViewBag.Title = "培训薪资查看";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}

<style type="text/css">
    tbody table td {
        padding: 0;
    }

    .radio + .radio {
        margin-top: 10px;
    }

    .table-bordered {
        border-radius: 4px;
    }
</style>

<link href="~/Content/js/bootstrap-duallistbox/bootstrap-duallistbox.min.css" rel="stylesheet" />

<form id="form1">

    <div class="box-header">
        <h3 class="box-title">培训薪资查看</h3>
        <div class="row margin">
          <div class="row">
			<div class="col-xs-12 col-sm-6">
				<div class="input-group formValue margin-bottom">
					<span class="input-group-addon no-border">分公司：</span>
					<span id="company_name" class="form-control "></span>
				</div>
			</div>
            <div class="col-xs-12 col-sm-6">
                <div class="input-group margin-bottom">
                    <span class="input-group-addon no-border"> 生效时间</span>
                    <span id="effect_date" class="form-control"></span>
                </div>
            </div>          
		  </div>            
        </div>
    </div>

    <div class="box-body" style="padding-bottom:0px;" id="trainer">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-lg-12">
                <div class="box box-warning">
                    <div class="box-header">
                        <h3 class="box-title">培训师</h3>
                        <div class="box-tools">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                    </div>

                    <div class="box-body" style="background-color:#ecf0f5">
                        <div class="row">                   
                            <div class="col-xs-12 col-sm-12 col-lg-12">
                                <div class="box box-widget">
                                    <div class="box-header with-border">
                                        <h3 class="box-title">KPI</h3>
                                        <div class="box-tools">
                                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                                <i class="fa fa-minus"></i>
                                            </button>
                                        </div>
                                    </div>
                                    <div class="box-body">
                                        <table class="table table-bordered text-center">
                                            <tr>
                                                <th class="bg-gray-light" width="10%">考核项目</th>
                                                <th class="bg-gray-light" width="13%">考核内容</th>
                                                <th class="bg-gray-light" width="12%">建议金额</th>
                                                <th class="bg-gray-light" width="15%">标准金额</th>
												<th class="bg-gray-light" width="15%">考核标准</th>
                                                <th class="bg-gray-light" width="35%">KPI（元）<span style="color:red">只罚不奖</span></th>
                                            </tr>

                                            <tr>
												<th rowspan="2" style="text-align:center;vertical-align:middle;">V雪球</th>
												<th style="text-align:center;vertical-align:middle;">活跃人数</th>
                                                <td>
                                                    <div class="input-group">
                                                        <span class="form-control text-center" id="snowball_number_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="snowball_number_standard" name="snowball_number_standard"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">元/人</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="snowball_number_assess" name="snowball_number_assess"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">人</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="snowball_number_kpi">(实际活跃人数 - 考核标准) * 标准金额</span>
                                                </td>
                                            </tr>
                                            <tr>
												<th style="text-align:center;vertical-align:middle;">V雪球转化率</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="snowball_ratio_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
														<div class="formValue">
															<span class="form-control text-center required number"  id="snowball_ratio_standard" name="snowball_ratio_standard"></span>
														</div>	
															<span class="input-group-addon no-border">元</span>
													</div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
														<div class="formValue">
															<span class="form-control text-center required number" id="snowball_ratio_assess" name="snowball_ratio_assess"></span>
														 </div>
															<span class="input-group-addon no-border">%</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="snowball_ratio_kpi">[ 1 - ( 考核标准 - 实际转化率 ) * 5 ] * 标准金额</span>
                                                </td>
                                            </tr>
                                            
											<tr>
												<th rowspan="2" style="text-align:center;vertical-align:middle;">导购服务</th>
												<th style="text-align:center;vertical-align:middle;">人均产值</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="shopguide_average_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="shopguide_average_standard" name="shopguide_average_standard"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="shopguide_average_assess" name="shopguide_average_assess"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">台</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="shopguide_average_kpi">(导购产生销量 / 产生销量导购人数) / 考核标准 * 100% * 标准金额</span>
                                                </td>
                                            </tr>
                                            <tr>
												<th style="text-align:center;vertical-align:middle;">导购离职率</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="shopguide_resign_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
															<span class="form-control text-center required number"  id="shopguide_resign_standard" name="shopguide_resign_standard"></span>
                                                        </div>
															<span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
															<span class="form-control text-center required number" id="shopguide_resign_assess" name="shopguide_resign_assess"></span>
                                                        </div>
															<span class="input-group-addon no-border">%</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="shopguide_resign_kpi">{ 1 - [ 离职人数 / (月底人数 + 离职人数) - 考核标准 ] * 5 } * 标准金额</span>
                                                </td>
                                            </tr>

											<tr>
												<th rowspan="2" style="text-align:center;vertical-align:middle;">产品培推</th>
												<th style="text-align:center;vertical-align:middle;">高端机占比</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="product_expensive_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="product_expensive_standard" name="product_expensive_standard"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="product_expensive_assess" name="product_expensive_assess"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">%</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="product_expensive_kpi">[ 1 - ( 考核标准 - 下属导购实销高端机占比 ) * 5 ] * 标准金额</span>
                                                </td>
                                            </tr>
                                            <tr>
												<th style="text-align:center;vertical-align:middle;">培训场次</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="product_train_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
															<span class="form-control text-center required number" id="product_train_standard" name="product_train_standard"></span>
                                                        </div>
															<span class="input-group-addon no-border">元/场</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
															<span class="form-control text-center required number" id="product_train_assess" name="product_train_assess"></span>
                                                        </div>
															<span class="input-group-addon no-border">场</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="product_train_kpi">( 实际场次 - 考核标准 ) * 标准金额</span>
                                                </td>
                                            </tr>

											<tr>
												<th rowspan="2" style="text-align:center;vertical-align:middle;">形象</th>
												<th style="text-align:center;vertical-align:middle;">形象执行效率</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="image_efficiency_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="image_efficiency_standard" name="image_efficiency_standard"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center">100</span>
                                                        <span class="input-group-addon no-border">分</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="image_efficiency_kpi">品牌部打分分值 / 100 * 标准金额</span>
                                                </td>
                                            </tr>
                                            <tr>
												<th style="text-align:center;vertical-align:middle;">形象罚款</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="image_fine_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
															<span class="form-control text-center required number" id="image_fine_standard" name="image_fine_standard"></span>
                                                        </div>
															<span class="input-group-addon no-border">元/家</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
															<span class="input-group-addon no-border">抽查</span>
                                                        <div class="formValue">
															<span class="form-control text-center required number" id="image_fine_number" name="image_fine_number"></span>
                                                        </div>
															<span class="input-group-addon no-border">家</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="image_fine_kpi">不合格门店数 * 标准金额</span>
                                                </td>
                                            </tr>

											<tr>
												<th style="text-align:center;vertical-align:middle;">必选项</th>
												<th style="text-align:center;vertical-align:middle;">终端经理打分</th>
                                                <td>
													<div class="input-group">
                                                        <span class="form-control text-center" id="manager_scoring_advice"></span>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td>
                                                    <div class="input-group">
                                                        <div class="formValue">
                                                            <span class="form-control text-center required number" id="manager_scoring_standard" name="manager_scoring_standard"></span>
                                                        </div>
                                                        <span class="input-group-addon no-border">元</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <div class="input-group">
                                                        <span class="form-control text-center">100</span>
                                                        <span class="input-group-addon no-border">分</span>
                                                    </div>
                                                </td>
                                                <td style="text-align:center;vertical-align:middle;">
                                                    <span id="manager_scoring_kpi">( 工作态度分值 + 效率分值 ) / 100 * 标准金额</span>
                                                </td>
                                            </tr>
                                        </table>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
			</div>
         </div>
	</div>
			
	<div class="box-body">
        <div class="row">
            <div class="col-xs-12 col-sm-12 col-lg-12">
				<div class="box box-success">
                    <div class="box-header">
                        <div class="row">
                            <div class="input-group col-sm-5 col-lg-3">
                                <h3 class="box-title input-group-addon no-border">交通补贴</h3>
                                <div class="checkbox input-group-addon no-border">
                                    <label class="text-red">
                                        <input type="checkbox" id="assessment_setting" disabled/>将所有个人设置重置为此方案
                                    </label>
                                </div>
                            </div>
                        </div>
                        <div class="box-tools">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        <div class="row margin-bottom">
                            <div class="col-sm-2 col-lg-1"><span class="form-control no-border">补贴金额</span></div>
                            <div class="col-sm-3 col-lg-3">
                                <div class="input-group">
									<div class="formValue">
										<span class="form-control text-center required number" id="traffic_subsidy"></span>
									</div>
										<span class="input-group-addon ">元</span>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
			</div>
		</div>
	</div>

<div class="box-body">
  <div class="row">
	<div class="col-xs-12 col-sm-12 col-lg-12">
            <div class="box box-default  collapsed-box">
                <div class="box-header widt-border">
                    <h3 class="box-title">审批</h3>
                    <div class="box-tools pull-right">
                        <button type="button" class="btn btn-box-tool" data-widget="collapse">
                            <i class="fa fa-plus"></i>
                        </button>
                    </div>
                </div>
                <div class="box-body">
                    <table class="table table-bordered text-center" id="approve">
                        <tbody>
                            <tr>
                                <th width="150px">操作类型</th>
                                <th width="150px">操作人</th>
                                <th width="150px">操作人职位</th>
                                <th width="200px">时间</th>
                                <th width="200px">结果</th>
                                <th width="200px">意见</th>
                            </tr>
                            <tr>
                                <td>发起申请</td>
                                <td><span id="creator_name"></span></td>
                                <td><span id="creator_position_name"></span></td>
                                <td><span id="create_time"></span></td>
                                <td><span id="">--</span></td>
                                <td><span id="">--</span></td>
                            </tr>
                        </tbody>
                    </table>
                </div>
            </div>
		</div>
	 </div>
 </div>

</form>

<div class="box-footer">
    <button type="submit" class="btn btn-default pull-right" onclick="javascript:history.back(-1);">返回</button>
</div>

<script src="~/Content/js/detail/FinancialAccounting/PositionSalary/trainer_show.js"></script>
