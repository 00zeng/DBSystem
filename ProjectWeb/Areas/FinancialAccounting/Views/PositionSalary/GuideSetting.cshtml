                            <div class="box-header with-border">
                                <h3 class="box-title">保底工资</h3>
                                <div class="box-tools">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">
                                <div class="row margin-bottom">
                                    <div class="col-sm-6 col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-addon  no-border">保底工资</span>
                                            <div class="formValue">
                                                <input class="form-control text-center required number" id="guide_salary_base" name="guide_salary_base">
                                            </div>
                                            <span class="input-group-addon ">元/月</span>
                                        </div>
                                    </div>
                                    <div class="col-sm-6 col-lg-3">
                                        <div class="input-group">
                                            <span class="input-group-addon  no-border">全勤奖</span>
                                            <div class="formValue">
                                                <input class="form-control text-center required number" id="" name="">
                                            </div>
                                            <span class="input-group-addon ">元/月</span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @*年终奖*@
