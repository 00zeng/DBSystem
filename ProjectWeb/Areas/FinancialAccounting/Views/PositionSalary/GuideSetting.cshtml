                            <div class="box-header with-border">
                                <h3 class="box-title">星级制</h3>
                                <div class="box-tools">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">

                                <table class="table table-bordered text-center formValue" id="baseStarlevel_table">
