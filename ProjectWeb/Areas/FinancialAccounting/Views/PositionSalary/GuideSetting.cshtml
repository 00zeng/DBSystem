                            <div class="box-header with-border">
                                <h3 class="box-title">浮动底薪</h3>
                                <div class="box-tools">
                                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                                        <i class="fa fa-minus"></i>
                                    </button>
                                </div>
                            </div>
                            <div class="box-body">
                                <table class="table table-bordered text-center" id="float_salary_tr">
