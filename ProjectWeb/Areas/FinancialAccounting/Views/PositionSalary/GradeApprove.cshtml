@*职等薪资导入*@
@{
    ViewBag.Title = "职等薪资导入查看";
    Layout = "~/Views/Shared/_Form_dinf.cshtml";
}
<link href="~/Content/css/dinf-table.css" rel="stylesheet" />
<form id="form1">
    <header class="panel-heading">
        <span style="color:#ffffff">职等薪资导入查看</span>
    </header>
    <div class="panel-body" style="background:white;">
        <div class="row">
            <div class="col-xs-1">
            </div>
            <div class="col-xs-10" style="min-width:1000px">

                <div class="form">
                    <div class="main-title main-title2">
                        <div class="main-title2-type">
                            机构：<span id="company_name" style="margin-right:60px"></span>
                            生效时间：<span id="effect_date" class="format_date_ym" style="margin-right:60px"></span>
                            状态：<span id="cur_status" style="margin-right:60px"></span>
                            导入文件名：<span id="file_name"></span>
                        </div>
                    </div>


                    <div class="main-section">
                        <div class="mid-title-wrap">
                            <div class="mid-title">导入结果</div>
                        </div>
                        <table id="tb_table" style="table-layout:fixed;"></table>
                    </div>

                    <div class="mid-title-wrap">
                        <div class="mid-title">审批</div>
                    </div>
                    <table class="normal-approve" id="approve">
                        <tr>
                            <th>操作类型</th>
                            <th>操作人</th>
                            <th>操作人职位</th>
                            <th>时间</th>
                            <th>结果</th>
                            <th>意见</th>
                        </tr>
                        <tr>
                            <td>文件导入</td>
                            <td><span id="creator_name"></span></td>
                            <td><span id="creator_position_name"></span></td>
                            <td><span id="create_time"></span></td>
                            <td  style="background:LightGray"></td>
                            <td  style="background:LightGray"></td>
                        </tr>
                        <tr>
                            <td>财务经理审批</td>
                            <td><span id="approve_name"></span></td>
                            <td><span id="approve_position_name"></span></td>
                            <td><span id="approve_time"></span></td>
                            <td>
                                <input name="status" type="radio" value="1" checked>通过 &nbsp;&nbsp;
                                <input name="status" type="radio" value="-1">不通过
                            </td>
                            <td><input id="approve_note" style="width:100%" /></td>
                        </tr>
                    </table>
                </div>
                <div class="main-submit">
                    <a class="btn-return" id="btnReturn" href="javascript:history.go(-1);"> 返回 </a>
                    <a class="btn-submit" id="btnSubmit" onclick="submitForm()">提交</a>
                </div>
            </div>
        </div>
    </div>
</form>
<script src="~/Content/js/detail/FinancialAccounting/PositionSalary/gradeApprove.js"></script>