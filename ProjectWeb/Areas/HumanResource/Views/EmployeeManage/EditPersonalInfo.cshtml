@{
    ViewBag.Title = "员工信息修改";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}

<div class="panel-body" style="background: white;">
    <form id="form1">
        <div class="box box-danger">
            <div class="box-header widt-border">
                <h3 class="box-title">个人信息修改：</h3>
                <span id="work_number" class="label-default"></span>
                <span id="name" class="label-default"></span>               
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body">
                <div class="row">                   
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">电话号码<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="phone" name="phone" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">微信号<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="wechat" name="wechat" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">证件类别<span class="text-red">&nbsp;*</span></span>
                            <select id="identity_type" name="identity_type" class="form-control required">
                                <option value="居民身份证" selected>居民身份证</option>
                                <option value="护照">护照</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">证件号码<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="identity" name="identity" class="form-control required" onblur="CheckIDCard(this);">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">证件地址<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="identity_address" name="identity_address" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom">
                            <span class="input-group-addon">证件有效期<span class="text-red">&nbsp;*</span></span>
                            <div class="formValue">
                                <input type="text" id="identity_effect" name="identity_effect" class="form-control required format_date" readonly="readonly" onfocus="WdatePicker({ maxDate: '#F{$dp.$D(\'identity_expire\')}' });" onblur="CheckValid(this)" />
                            </div>
                            <span class="input-group-addon">-</span>
                            <div class="formValue">
                                <input type="text" id="identity_expire" name="identity_expire" class="form-control required format_date" readonly="readonly" onfocus="WdatePicker({ minDate: '#F{$dp.$D(\'identity_effect\')}' });" onblur="CheckValid(this)" />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">证件签发机关<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="identity_issue" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom ">
                            <span class="input-group-addon">性别<span class="text-red">&nbsp;*</span></span>
                            <div class="form-control radio" style="margin-top:0px">
                                <label class="col-sm-6 ">
                                    <input id="gender" name="gender" type="radio" value="0">女
                                </label>
                                <label class="col-sm-6 ">
                                    <input name="gender" type="radio" value="1">男
                                </label>
                            </div>  
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">出生日期<span class="text-red">&nbsp;*</span></span>
                            <input id="birthdate" name="birthdate" type="text" class="form-control required format_date" readonly="readonly" onfocus="WdatePicker({ dateFmt: 'yyyy-MM-dd',maxDate:'%y-%M-%d' });">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">年龄<span class="text-red">&nbsp;*</span></span>
                            <input id="age" name="age" type="text" class="form-control required" />
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">生日<span class="text-red">&nbsp;*</span></span>
                            <input id="birthday" name="birthday" type="text" class="form-control required format_date_md" readonly="readonly" onfocus="WdatePicker({ dateFmt: 'MM-dd' });">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">生日类型<span class="text-red">&nbsp;*</span></span>
                            <select id="birthday_type" class="form-control required">
                                <option value="1">农历</option>
                                <option value="2" selected>阳历</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">国家或地区<span class="text-red">&nbsp;*</span></span>
                            <select id="country" class="form-control required">
                                <option value="中国" selected>中国</option>
                                <option value="中国香港">中国香港</option>
                                <option value="中国澳门">中国澳门</option>
                                <option value="中国台湾">中国台湾</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">家庭住址<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="address" name="address" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">籍贯<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="native" name="native" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">民族<span class="text-red">&nbsp;*</span></span>
                            <select id="nation" name="nation" class="form-control required">
                                <option value="汉族" selected>汉族</option>
                                <option value="蒙古族">蒙古族</option>
                                <option value="回族">回族</option>
                                <option value="藏族">藏族</option>
                                <option value="维吾尔族">维吾尔族</option>
                                <option value="苗族">苗族</option>
                                <option value="彝族">彝族</option>
                                <option value="壮族">壮族</option>
                                <option value="布依族">布依族</option>
                                <option value="朝鲜族">朝鲜族</option>
                                <option value="满族">满族</option>
                                <option value="侗族">侗族</option>
                                <option value="瑶族">瑶族</option>
                                <option value="白族">白族</option>
                                <option value="土家族">土家族</option>
                                <option value="哈尼族">哈尼族</option>
                                <option value="哈萨克族">哈萨克族</option>
                                <option value="傣族">傣族</option>
                                <option value="黎族">黎族</option>
                                <option value="傈僳族">傈僳族</option>
                                <option value="佤族">佤族</option>
                                <option value="畲族">畲族</option>
                                <option value="高山族">高山族</option>
                                <option value="拉祜族">拉祜族</option>
                                <option value="水族">水族</option>
                                <option value="东乡族">东乡族</option>
                                <option value="纳西族">纳西族</option>
                                <option value="景颇族">景颇族</option>
                                <option value="柯尔克孜族">柯尔克孜族</option>
                                <option value="土族">土族</option>
                                <option value="达斡尔族">达斡尔族</option>
                                <option value="仫佬族">仫佬族</option>
                                <option value="羌族">羌族</option>
                                <option value="布朗族">布朗族</option>
                                <option value="撒拉族">撒拉族</option>
                                <option value="毛南族">毛南族</option>
                                <option value="仡佬族">仡佬族</option>
                                <option value="锡伯族">锡伯族</option>
                                <option value="阿昌族">阿昌族</option>
                                <option value="普米族">普米族</option>
                                <option value="塔吉克族">塔吉克族</option>
                                <option value="怒族">怒族</option>
                                <option value="乌孜别克族">乌孜别克族</option>
                                <option value="俄罗斯族">俄罗斯族</option>
                                <option value="鄂温克族">鄂温克族</option>
                                <option value="德昂族">德昂族</option>
                                <option value="保安族">保安族</option>
                                <option value="裕固族">裕固族</option>
                                <option value="京族">京族</option>
                                <option value="塔塔尔族">塔塔尔族</option>
                                <option value="独龙族">独龙族</option>
                                <option value="鄂伦春族">鄂伦春族</option>
                                <option value="赫哲族">赫哲族</option>
                                <option value="门巴族">门巴族</option>
                                <option value="珞巴族">珞巴族</option>
                                <option value="基诺族">基诺族</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">政治面貌<span class="text-red">&nbsp;*</span></span>
                            <select id="political" class="form-control required">
                                <option value="中共党员">中共党员</option>
                                <option value="团员">团员</option>
                                <option value="群众">群众</option>
                                <option value="民主党派">民主党派</option>
                                <option value="无党派人士">无党派人士</option>
                                <option value="其他">其他</option>
                            </select>

                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">学历<span class="text-red">&nbsp;*</span></span>
                            <select id="education" class="form-control required">
                                <option value="小学">小学</option>
                                <option value="初中">初中</option>
                                <option value="高中">高中</option>
                                <option value="中专">中专</option>
                                <option value="大专">大专</option>
                                <option value="本科">本科</option>
                                <option value="硕士">硕士</option>
                                <option value="博士">博士</option>
                                <option value="其他">其他</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">专业<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="profession" name="profession" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">毕业学校<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="graduation_school" name="graduation_school" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">毕业日期<span class="text-red">&nbsp;*</span></span>
                            <input id="graduation_date" name="graduation_date" type="text" class="form-control required format_date" readonly="readonly" onfocus="WdatePicker();">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">婚姻状况<span class="text-red">&nbsp;*</span></span>
                            <select id="marriage" class="form-control required">
                                <option value="0">未婚</option>
                                <option value="1">已婚</option>
                                <option value="2">离异</option>
                                <option value="3">丧偶</option>
                                <option value="4">其他</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">子女<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="child_count" name="child_count" class="form-control required">
                            <span class="input-group-addon">个</span>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">紧急联系人<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="emergency_contact" name="emergency_contact" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">紧急联系电话<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="emergency_contact_phone" name="emergency_contact_phone" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom">
                            <span class="input-group-addon">健康证有效期<span class="text-red">&nbsp;*</span></span>
                            <div class="formValue">
                                <input type="text" id="health_start" name="health_start" class="form-control required format_date" readonly="readonly" onfocus="WdatePicker({ maxDate: '#F{$dp.$D(\'health_expire\')}' });" onblur="CheckValid(this)" />
                            </div>
                            <span class="input-group-addon">-</span>
                            <div class="formValue">
                                <input type="text" id="health_expire" name="health_expire" class="form-control required format_date" readonly="readonly" onfocus="WdatePicker({ minDate: '#F{$dp.$D(\'health_start\')}' });" onblur="CheckValid(this)" />
                            </div>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">银行类别<span class="text-red">&nbsp;*</span></span>
                            <select id="bank_type" class="form-control required">
                                <option value="国有银行">国有银行</option>
                                <option value="股份制商业银行">股份制商业银行</option>
                                <option value="城商行">城商行</option>
                                <option value="农信行">农信行</option>
                                <option value="邮储银行">邮储银行</option>
                                <option value="合作银行">合作银行</option>
                                <option value="其他">其他</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">开户行<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="bank_name" name="bank_name" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">银行账号<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="bank_account" name="bank_account" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">父母银行卡开户银行<span class="text-red">&nbsp;*</span></span>
                            <select id="parents_bank" class="form-control required">
                                <option value="国有银行">国有银行</option>
                                <option value="股份制商业银行">股份制商业银行</option>
                                <option value="城商行">城商行</option>
                                <option value="农信行">农信行</option>
                                <option value="邮储银行">邮储银行</option>
                                <option value="合作银行">合作银行</option>
                                <option value="其他">其他</option>
                            </select>
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">父母银行卡号<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="parents_bankaccount" name="parents_bankaccount" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom formValue">
                            <span class="input-group-addon">父母联系电话<span class="text-red">&nbsp;*</span></span>
                            <input type="text" id="parents_phone" name="parents_phone" class="form-control required">
                        </div>
                    </div>
                    <div class="col-xs-12 col-sm-6 col-lg-4">
                        <div class="input-group margin-bottom">
                            <span class="input-group-addon">备注</span>
                            <input type="text" id="note" name="note" class="form-control">
                        </div>
                    </div>
                </div>
            </div>
        </div>      
    </form>
</div>
<div class="box-footer">
    <button type="submit" class="btn btn-primary pull-right" onclick="submitForm()">提交</button>
    <a class="btn btn-default pull-right" href="javascript:history.go(-1);"> 返回 </a>
</div>
<script src="~/Content/js/deepinf/dinf-input.js"></script>
<script src="~/Content/js/detail/humanResource/employeeManage/edit_personal_info.js"></script>
