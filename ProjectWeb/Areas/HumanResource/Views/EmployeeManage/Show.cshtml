@{
    ViewBag.Title = "员工信息 ";
    Layout = "~/Views/Shared/_Form_v2.cshtml";
}
<div class="panel-body" style="background: white;">
    <form id="form1">

        <div class="box box-warning ">
            <div class="box-header widt-border">
                <h3 class="box-title">职务信息</h3>
                <div class="box-tools pull-right">
                    <button type="button" class="btn btn-box-tool" onclick="EditJobInfo()">
                        <i class="fa fa-pencil" title="修改职务信息"></i>
                    </button>
                    <button type="button" class="btn btn-box-tool" data-widget="collapse">
                        <i class="fa fa-minus"></i>
                    </button>
                </div>
            </div>
            <div class="box-body">
                <div class="row">
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>工号</dt>
                        <dd>
                            <span id="work_number"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>姓名</dt>
                        <dd>
                            <span id="name" name="name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>v2姓名</dt>
                        <dd>
                            <span id="name_v2" name="name_v2"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>所属机构</dt>
                        <dd>
                            <span id="company_linkname" name="company_linkname"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>部门</dt>
                        <dd>
                            <span id="dept_name" name="dept_name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>职位</dt>
                        <dd>
                            <span id="position_name" name="position_name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>职级</dt>
                        <dd>
                            <span id="grade" name="grade"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal" hidden id="areaL1Wrap">
                        <dt>经理片区</dt>
                        <dd>
                            <span id="area_l1_name" name="area_l1_name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal" hidden id="areaL2Wrap">
                        <dt>业务片区</dt>
                        <dd>
                            <span id="area_l2_name" name="area_l2_name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal" hidden id="distributorWrap">
                        <dt>经销商</dt>
                        <dd>
                            <span id="distributor_name" name="distributor_name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>上级主管姓名</dt>
                        <dd>
                            <span id="supervisor_name" name="supervisor_name"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>雇员类别</dt>
                        <dd>
                            <span id="emp_category" name="emp_category"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>入职时间</dt>
                        <dd>
                            <span id="entry_date" name="entry_date" class="format_date"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>工作地点</dt>
                        <dd>
                            <span id="work_addr" name="work_addr"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>纳税单位</dt>
                        <dd>
                            <span id="tax_unit" name="tax_unit"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>是否已购买社保</dt>
                        <dd>
                            <span id="insurance_paid" name="insurance_paid"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>合同签署日期</dt>
                        <dd>
                            <span id="cur_contract_sign" name="cur_contract_sign" class="format_date"></span>
                        </dd>
                    </dl>
                    <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                        <dt>合同到期日期</dt>
                        <dd>
                            <span id="cur_contract_expire" name="cur_contract_expire" class="format_date"></span>
                        </dd>
                    </dl>
                    <div id="guideSpecificWrap" hidden>
                        <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                            <dt>导购员类别</dt>
                            <dd>
                                <span id="guide_category" name="guide_category"></span>
                            </dd>
                        </dl>
                        <dl class="col-xs-12 col-sm-6 col-lg-4 dl-horizontal">
                            <dt>介绍人</dt>
                            <dd>
                                <span id="introducer_name" name="introducer_name"></span>
                            </dd>
                        </dl>
                    </div>
                </div>

            </div>
        </div>
        <div class="box box-danger collapsed-box">
