@{
    ViewBag.Title = "道本工资管理系统";
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>道本工资管理系统</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.7 -->
    <link rel="stylesheet" href="~/Content/js/bootstrap/css/bootstrap.min.css">
        <!-- Font Awesome -->
    <link rel="stylesheet" href="~/Content/css/font-awesome.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="~/Content/js/adminlte/css/AdminLTE.min.css">
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="~/Content/js/adminlte/css/skins/skin-blue.min.css">
    <link rel="stylesheet" href="~/Content/css/dinf-home.css">
    <link rel="stylesheet" href="~/Content/css/dinf-index.css">
    <link rel="stylesheet" href="~/Content/css/dinf-common.css">

    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="/Home/index" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini">道本</span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg">企业信息管理</span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav id="navbar-static-top" class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                </a>
                <ol class="breadcrumb">
                    <li><i class="fa fa-home"></i>&nbsp;&nbsp;<span id="nav1stTitle">主页</span></li>
                    <li><span id="nav2ndTitle"></span></li>
                    <li><span id="nav3rdTitle"></span></li>
                </ol>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        <!-- Notifications: style can be found in dropdown.less -->
                        <li class="dropdown notifications-menu" onmouseover="showsub(this)" onmouseout="hidesub(this)">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-bell-o"></i>
                                <span class="label label-warning"><span id="noticeLength"></span></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">你有<span id="noticeLength1"></span>条消息</li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu" id="noticeTitle" name="noticeTitle">
									</ul>
                                </li>
                               @* <li class="footer"><a href="#">查看所有消息</a></li>*@
                            </ul>
                        </li>
                        <!-- Tasks: style can be found in dropdown.less -->
                        <li class="dropdown tasks-menu" onmouseover="showsub(this)" onmouseout="hidesub(this)">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <i class="fa fa-flag-o"></i>
                                <span class="label label-danger"><span id="taskLength"></span></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li class="header">你有<span id="taskLength1"></span>个任务</li>
                                <li>
                                    <!-- inner menu: contains the actual data -->
                                    <ul class="menu" id="taskTitle" name="taskTitle">
                                    </ul>
                                </li>
                                @*<li class="footer">
                                    <a href="#">查看所有任务</a>
                                </li>*@
                            </ul>
                        </li>
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu" onmouseover="showsub(this)" onmouseout="hidesub(this)">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <img src="~/Content/js/adminlte/img/user2-160x160.jpg" class="user-image" alt="User Image">
                                <span class="hidden-xs">@(ViewBag.account)</span>
                            </a>
                            <ul class="dropdown-menu">
                                <!-- User image -->
                                <li class="user-header">
                                    <img src="~/Content/js/adminlte/img/user2-160x160.jpg" class="img-circle" alt="User Image">
                                    <p>
                                        @(ViewBag.position_name) - @(ViewBag.dept_name)
                                        <small>@(ViewBag.role_name)</small>
                                    </p>
                                </li>
                                <!-- Menu Body -->
                                <!-- Menu Footer-->
                                <li class="user-footer">
                                    <div class="pull-left">
                                        <button id="changePasswore" name="changePasswore" class="btn btn-default btn-flat">修改密码</button>
                                    </div>
                                    <div class="pull-right">
                                        <button  id="btnCancel" name="btnCancel" class="btn btn-default btn-flat">退出</button>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </nav>
        </header>
        <!-- Left side column. contains the logo and sidebar -->
        <aside class="main-sidebar" style="overflow: hidden">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar" style="overflow-y: auto">
                <!-- Sidebar user panel -->
                <div class="user-panel">
                    <div class="pull-left image">
                        <img src="~/Content/js/adminlte/img/user2-160x160.jpg" class="img-circle" alt="头像">
                    </div>
                    <div class="pull-left info">
                        <p>@(ViewBag.account)</p>
                        <a href="#"><i class="fa fa-circle text-success"></i> 在线</a>
                    </div>
                </div>
                <!-- search form -->
                <!-- /.search form -->
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul id="sidebar-menu" class="sidebar-menu" data-widget="tree"></ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <div class="content-tabs">
                <button class="roll-nav roll-left tabLeft">
                    <i class="fa fa-backward"></i>
                </button>
                <nav class="page-tabs menuTabs">
                    <div class="page-tabs-content" style="margin-left: 0px;">
                        <a href="javascript:;" class="menuTab active" data-cookie="201603" data-id="/Home/Home" data-nav1="" data-nav2="" data-nav3=""  >欢迎首页</a>
                        @*<a href="javascript:;" class="menuTab" data-id="/Home/About" style="padding-right: 15px;">平台介绍</a>*@
                    </div>
                </nav>
                <button class="roll-nav roll-right tabRight">
                    <i class="fa fa-forward" style="margin-left: 3px;"></i>
                </button>
                <div class="btn-group roll-nav roll-right">
                    <div class="dropdown tabClose tab-pointer" data-toggle="dropdown" style="background-color:white">
                        页签操作<i class="fa fa-caret-down" style="padding-left: 3px;"></i>
                    </div>
                    <ul class="dropdown-menu dropdown-menu-right">
                        <li><a class="tabReload" href="javascript:void();">刷新当前</a></li>
                        <li><a class="tabCloseCurrent" href="javascript:void();">关闭当前</a></li>
                        <li><a class="tabCloseAll" href="javascript:void();">全部关闭</a></li>
                        <li><a class="tabCloseOther" href="javascript:void();">除此之外全部关闭</a></li>
                    </ul>
                </div>
                <button class="roll-nav roll-right fullscreen"><i class="fa fa-arrows-alt"></i></button>
            </div>
            @*<section id="content-header" class="content-header">
                <h1>
                    Dashboard
                    <small>Version 2.0</small>
                </h1>
                <ol class="breadcrumb">
                    <li><a href="#"><i class="fa fa-home"></i><span id="nav1ndTitle">主页</span></a></li>
                    <li class="active"><span id="nav2ndTitle"></span></li>
                </ol>
            </section>*@
            <section class="content">
                <div class="box box-primary mainContent" style="margin-bottom: 0">
                    <iframe class="Daoben_iframe" id="myFrameId" name="myFrameId" onscroll="false" style="width: 100%; height:100%;overflow: scroll; border:0px;"></iframe>
                    <div id="loadingPage" class="overlay" hidden>
                        <i class="fa fa-refresh fa-spin"></i>
                    </div>
                </div>
            </section>
            @*<div class="content-iframe" style="background-color: #212f41; overflow: hidden;">
                    <div class="mainContent" id="content-main" style="margin: 10px; padding: 0;">
                        <iframe class="Daoben_iframe" id="myFrameId" name="myFrameId" src="HomePage" onscroll="false" style="width: 100%; height:100%;overflow: scroll; border: 1px solid #5a5f5f; border-radius:5px"></iframe>
                    </div>
                </div>*@
        </div>
        <!-- /.content-wrapper -->
        @*<footer class="main-footer">
                <div class="pull-right hidden-xs">
                    <b>Version</b> 2.4.0
                </div>
                <strong>Copyright &copy; 2014-2016 <a href="https://adminlte.io">Almsaeed Studio</a>.</strong> All rights
                reserved.
            </footer>*@
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>
    <!-- ./wrapper -->
    <!-- jQuery 3 -->
    <script src="~/Content/js/jquery/jquery-3.1.1.js"></script>
    <script>
    </script>
    <!-- Bootstrap 3.3.7 -->
    <script src="~/Content/js/bootstrap/js/bootstrap.min.js"></script>
    <!-- AdminLTE App -->
    <script src="~/Content/js/adminlte/js/adminlte.min.js"></script>
    <script src="~/Content/js/layer/layer.js"></script>
    <script src="~/Content/js/detail/Home/authority_init.js"></script>
    <script src="~/Content/js/detail/Home/index.js"></script>
    <script src="~/Content/js/deepinf/dinf-base-preload.js"></script>
    <script src="~/Content/js/deepinf/dinf-base-postload.js"></script>
    <script src="~/Content/js/deepinf/dinf-indextab.js"></script>
</body>
</html>

